<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- First diagonal line -->
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="rectangle">
                <size
                    android:width="2dp"
                    android:height="20dp" />
                <solid android:color="#000000"/> <!-- Change color as needed -->
            </shape>
        </rotate>
    </item>

    <!-- Second diagonal line -->
    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="rectangle">
                <size
                    android:width="2dp"
                    android:height="20dp" />
                <solid android:color="#000000"/> <!-- Change color as needed -->
            </shape>
        </rotate>
    </item>

</layer-list>
